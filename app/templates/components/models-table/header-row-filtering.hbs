<tr>
  {{#each processedColumns as |column|}}
    {{#if column.isVisible}}
      <th class="{{classes.theadCell}} {{unless column.useFilter classes.theadCellNoFiltering}}">
        {{#if column.useFilter}}
          {{#if column.filterWithSelect}}
            {{#if column.filterDropdownTemplate}}
              {{partial column.filterDropdownTemplate}}
            {{else}}
              {{partial "components/models-table/filter-select"}}
            {{/if}}
          {{else}}
            {{input type="text" value=column.filterString class="form-control" enter="" focus-out="changeFilterString"}}
          {{/if}}
        {{else}}
          &nbsp;
        {{/if}}
      </th>
    {{/if}}
  {{/each}}
</tr>